<div class='roster-row {{if volunteer.isCheckedIn 'checked-in'}} test-roster-row'>
    <div class="volunteer-name test-volunteer-member-name">{{volunteer.name}}</div>
    <div class="volunteer-status test-volunteer-member-status">{{volunteer.status}}</div>
    <div class="volunteer-number test-volunteer-time-checked-in">{{moment-format volunteer.timeCheckedIn 'h:mm A'}}</div>
</div>